<p id="notice"><%= notice %></p>

<h1>Word Srcs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Create person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_srcs.each do |word_src| %>
      <tr>
        <td><%= word_src.name %></td>
        <td><%= word_src.create_person %></td>
        <td><%= link_to 'Show', word_src %></td>
        <td><%= link_to 'Edit', edit_word_src_path(word_src) %></td>
        <td><%= link_to 'Destroy', word_src, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word Src', new_word_src_path %>
